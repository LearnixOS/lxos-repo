{
    "pkgname": "mpc",
    "version": "1.3.1",
    "source": ["https://ftp.gnu.org/gnu/mpc/mpc-1.3.1.tar.gz"],
    "build": [
        "cd mpc-1.3.1.tar.gz",
        "./configure --prefix=/usr --disable-static --docdir=/usr/share/doc/mpc-1.3.1",
        "make",
        "make html",
        "make install",
        "make install-html"
    ]
}
