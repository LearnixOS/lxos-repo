{
    "pkgname": "bison",
    "version": "3.8.2",
    "source": ["https://ftp.gnu.org/gnu/bison/bison-3.8.2.tar.xz"],
    "build": [
        "tar -xvf bison-3.8.2.tar.xz",
        "cd bison-3.8.2",
        "./configure --prefix=/usr --docdir=/usr/share/doc/bison-3.8.2",
        "make",
        "make install"
    ]
}
