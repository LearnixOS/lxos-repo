{
    "pkgname": "gperf",
    "version": "3.1",
    "source": ["https://ftp.gnu.org/gnu/gperf/gperf-3.1.tar.gz"],
    "build": [
        "tar -xvf gperf-3.1.tar.gz",
        "cd gperf-3.1",
        "./configure --prefix=/usr --docdir=/usr/share/doc/gperf-3.1",
        "make",
        "make install"
    ]
}
