{
    "pkgname": "automake",
    "version": "1.17",
    "source": ["https://ftp.gnu.org/gnu/automake/automake-1.17.tar.xz"],
    "build": [
        "tar -xvf automake-1.17.tar.xz",
        "cd automake-1.17",
        "./configure --prefix=/usr --docdir=/usr/share/doc/automake-1.17",
        "make",
        "make install"
    ]
}
