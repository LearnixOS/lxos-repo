{
    "pkgname": "mpfr",
    "version": "4.2.1",
    "source": ["https://ftp.gnu.org/gnu/mpfr/mpfr-4.2.1.tar.xz"],
    "build": [
        "tar -xvf mpfr-4.2.1.tar.xz",
        "cd mpfr-4.2.1",
        "./configure --prefix=/usr --disable-static --enable-thread-safe --docdir=/usr/share/doc/mpfr-4.2.1",
        "make",
        "make html",
        "make check",
        "make install",
        "make install-html"
    ]
}
