{
    "pkgname": "bash",
    "version": "5.2.32",
    "source": ["https://ftp.gnu.org/gnu/bash/bash-5.2.32.tar.gz"],
    "build": [
        "cd bash-5.2.32",
        "./configure --prefix=/usr --without-bash-malloc --with-installed-readline bash_cv_strtold_broken=no --docdir=/usr/share/doc/bash-5.2.32",
        "make",
        "make install"
    ]
}
