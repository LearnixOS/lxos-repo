{
    "pkgname": "markupsafe",
    "version": "2.1.5",
    "source": ["https://pypi.org/packages/source/M/MarkupSafe/MarkupSafe-2.1.5.tar.gz"],
    "build": [
        "cd MarkupSafe-2.1.5",
        "pip3 wheel -w dist --no-cache-dir --no-build-isolation --no-deps $PWD",
        "pip3 install --no-index --no-user --find-links dist Markupsafe"
    ]
}
